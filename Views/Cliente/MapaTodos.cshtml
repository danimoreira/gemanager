@model IEnumerable<GEPV.Domain.Entities.Cliente>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<input type="hidden" id="LocaisLatiLong" value="
       @foreach (var item in ViewBag.Locais) { 
            @item.Latilong
       }
       " />



<!-- Pointer events polyfill for old browsers, see https://caniuse.com/#feat=pointer -->
<link rel="stylesheet" href="https://openlayers.org/en/v4.6.5/css/ol.css" type="text/css">
<!-- The line below is only needed for old environments like Internet Explorer and Android 4.x -->
<script src="https://cdn.polyfill.io/v2/polyfill.min.js?features=requestAnimationFrame,Element.prototype.classList,URL"></script>



<style type="text/css">
    #map {
        width: 100%;
        height: 768px;
    }
</style>

<div class="d-block">

    <div class="row">
        <div class="col">
            <h1>Mapa de Clientes</h1>
        </div>
    </div>

    <div class="row">
        <div class="col">
            <div id="map" class="map"></div>
        </div>
    </div>

</div>


<script src="~/Scripts/openlayers/ol.js"></script>
<script src="~/Scripts/openlayers/mapa-cliente-todos.js"></script>